 .../Network/EthernetInterface.interface.yaml              | 8 ++++++++
 1 file changed, 8 insertions(+)

diff --git a/yaml/xyz/openbmc_project/Network/EthernetInterface.interface.yaml b/yaml/xyz/openbmc_project/Network/EthernetInterface.interface.yaml
index 348dd84..f72c772 100644
--- a/yaml/xyz/openbmc_project/Network/EthernetInterface.interface.yaml
+++ b/yaml/xyz/openbmc_project/Network/EthernetInterface.interface.yaml
@@ -123,6 +123,14 @@ properties:
       type: boolean
       description: >
           This is Enable/Disable IPv4. Default value is true.
+    - name: IPv6EnableStaticRtr
+      type: boolean
+      description: >
+          Boolean for enabling static router address in IPv6.
+    - name: IPv6StaticRtrAddr
+      type: string
+      description: >
+          IPv6 Static Router Address of ethernet interface.
 
 enumerations:
     - name: LinkLocalConf
